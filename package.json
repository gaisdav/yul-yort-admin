{
  "name": "yul-yort-admin",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "serve": "npm run serve-build && node server.js",
    "serve-build": "wireit",
    "gh-pages-build": "wireit",
    "build": "wireit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test": "react-scripts test",
    "testCoverage": "npm test --coverage",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "test-build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "react-scripts build"
    },
    "serve-build": {
      "command": "cross-env REACT_APP_BUILD_MODE=serve BUILD_PATH='serve-build' react-scripts build"
    },
    "gh-pages-build": {
      "command": "cross-env REACT_APP_BUILD_MODE=gh-pages BUILD_PATH='gh-pages' react-scripts build"
    },
    "test-build": {
      "dependencies": [
        "build",
        "serve-build",
        "gh-pages-build"
      ]
    }
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.25",
    "@types/react": "^16.14.15",
    "@types/react-dom": "^16.9.14",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "js-cookie": "^3.0.1",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.2",
    "react-router5": "^8.0.1",
    "react-scripts": "4.0.3",
    "router5": "^8.0.1",
    "router5-plugin-browser": "^8.0.1",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@types/js-cookie": "^3.0.1",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.0",
    "msw": "^0.35.0",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.3",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "wireit": "^0.4.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
